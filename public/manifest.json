  "icons": {
    "128": "xfolio256.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-TXGCp+wOzSsXfcahAikZHgOxIpn9fEzNuYBnvJBCp+k=';object-src 'self'",
  "permissions": [
    "tabs","<all_urls>"
  ]
}
